<odoo>
    <record id="view_warehouse_form_extend_users" model="ir.ui.view">
        <field name="name">stock.warehouse.form.view.users</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='code']" position="after">
                <field name="x_assigned_user_ids" widget="many2many_tags" placeholder="Select allowed users..."/>
                <field name="x_default_user_ids" widget="many2many_tags" placeholder="Select default users..."/>
            </xpath>
        </field>
    </record>
</odoo>